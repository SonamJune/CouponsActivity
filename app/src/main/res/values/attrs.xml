<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RippleBackground">
        <attr name="rb_color" format="color" />
        <attr name="rb_strokeWidth" format="dimension" />
        <attr name="rb_radius" format="dimension" />
        <attr name="rb_duration" format="integer" />
        <attr name="rb_rippleAmount" format="integer" />
        <attr name="rb_scale" format="float" />
        <attr name="rb_type" format="enum">
            <enum name="fillRipple" value="0" />
            <enum name="strokeRipple" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="StoriesProgressView">
        <attr name="progressCount" format="integer|reference" />
    </declare-styleable>

    <declare-styleable name="RobotoTextView">
        <attr name="typeface" />
    </declare-styleable>
    <attr name="hlv_dividerWidth" />
    <attr name="typeface" format="enum">
        <enum name="roboto_thin" value="0" />
        <enum name="roboto_thin_italic" value="1" />
        <enum name="roboto_light" value="2" />
        <enum name="roboto_light_italic" value="3" />
        <enum name="roboto_regular" value="4" />
        <enum name="roboto_italic" value="5" />
        <enum name="roboto_medium" value="6" />
        <enum name="roboto_medium_italic" value="7" />
        <enum name="roboto_bold" value="8" />
        <enum name="roboto_bold_italic" value="9" />
        <enum name="roboto_black" value="10" />
        <enum name="roboto_black_italic" value="11" />
        <enum name="roboto_condensed" value="12" />
        <enum name="roboto_condensed_italic" value="13" />
        <enum name="roboto_condensed_bold" value="14" />
        <enum name="roboto_condensed_bold_italic" value="15" />
        <enum name="anton" value="16" />
        <enum name="goutham_rounded" value="17" />
        <enum name="goutham_rounded_bold" value="18" />
        <enum name="goutham_rounded_bold_italic" value="19" />
        <enum name="goutham_rounded_italic" value="20" />
        <enum name="goutham_rounded_light" value="21" />
        <enum name="goutham_rounded_light_italic" value="22" />
        <enum name="goutham_rounded_medium" value="23" />
        <enum name="goutham_rounded_medium_italic" value="24" />
        <enum name="liberationmono_regular" value="25" />
        <enum name="purplle_icon" value="26" />
    </attr>

    <declare-styleable name="CardStack">
        <attr name="backgroundColor" format="color" />
    </declare-styleable>

    <attr name="bgColor" format="color"/>
    <attr name="textColor" format="color"/>
    <attr name="iconColor" format="color"/>
    <attr name="skeletonColor" format="color"/>

    <declare-styleable name="FaceCameraCaptureOverlayView">
        <attr name="overlay_color" />
    </declare-styleable>
    <attr name="overlay_color" format="color"/>
</resources>